{
    "name": "elnooronline/laravel-api-authentication",
    "description": "This package used to scaffold basic login, registration and reset password in restful api",
    "keywords": [
        "framework",
        "laravel",
        "api",
        "authentication",
        "passport"
    ],
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Ahmed Fathy",
            "email": "aliraqi.dev@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "laravel/framework": "5.5.*|5.6.*|5.7.*|5.8.*",
        "laravel/passport": "^7.0",
        "laravel-notification-channels/onesignal": "^1.2"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0",
        "mockery/mockery": "^1.2"
    },
    "autoload": {
        "psr-4": {
            "Elnooronline\\LaravelApiAuthentication\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Elnooronline\\LaravelApiAuthentication\\Providers\\ServiceProvider"
            ]
        }
    }
}
